{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev:next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["cdg1"],
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/socket",
      "destination": "/api/socket"
    }
  ]
}

